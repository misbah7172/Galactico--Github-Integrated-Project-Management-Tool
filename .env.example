# GitHub OAuth Application (Web App)
GITHUB_CLIENT_ID=your_web_app_client_id_here
GITHUB_CLIENT_SECRET=your_web_app_client_secret_here

# OAuth redirect URLs
OAUTH_REDIRECT_URI=https://your-domain.loca.lt/login/oauth2/code/github
OAUTH_EXTENSION_REDIRECT_URI=https://your-domain.loca.lt/api/auth/oauth-callback

# MySQL Database
DB_HOST=localhost
DB_PORT=3306
DB_NAME=autotrack
DB_USERNAME=root
DB_PASSWORD=your_database_password_here
DB_URL=jdbc:mysql://localhost:3306/autotrack?useSSL=false&allowPublicKeyRetrieval=true&serverTimezone=UTC

# GitHub API
GITHUB_API_BASE_URL=https://api.github.com